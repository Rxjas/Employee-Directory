{
  "name": "Employee-Directory",
  "version": "1.0.0",
  "description": "Easily see the information for your employees with this application.",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "seed": "node server/seeders/seed.js",
    "start:server": "nodemon server/server.js",
